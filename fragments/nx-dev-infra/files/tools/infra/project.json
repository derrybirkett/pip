{
  "name": "infra",
  "targets": {
    "up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose up -d dev db n8n"
      }
    },
    "down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose down"
      }
    },
    "logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose logs -f dev db n8n"
      }
    }
  }
}
